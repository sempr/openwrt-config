
# ipset config
ipset -N vpn iphash
iptables -t mangle -A PREROUTING -m set --match-set vpn dst -j MARK --set-mark 1
ip rule add fwmark 1 table 1

# enable gfwlist
cp /etc/vpn/gfwdomains.conf.gz /tmp/
gunzip /tmp/gfwdomains.conf.gz
ln -sf /tmp/gfwdomains.conf /etc/dnsmasq.d/
/etc/init.d/dnsmasq.d restart

# enable cron
/etc/init.d/cron restart

exit 0
